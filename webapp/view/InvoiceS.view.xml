			<PullToRefresh id="__refresh0"/>
			<SearchField id="__field0" placeholder="Search" width="100%" liveChange="onSearch"/>
			<List id="invoiceList" items="{/ocrInvoiceSet}" noDataText="Drop list items here">
				<items>
					<ObjectListItem id="__item3" type="Navigation" press="onInvoicePress" counter="0" markFavorite="false" markFlagged="false" showMarkers="false" title="{RdVendorName}" number="{Wrbtr}" numberUnit="{Waers}">
