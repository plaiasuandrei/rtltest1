<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:html="http://www.w3.org/1999/xhtml" controllerName="RTLtest1.controller.InvoiceS">
	<Page title="Invoices" showNavButton="true">
		<content>
			<PullToRefresh id="__refresh0"/>
			<SearchField width="100%" placeholder="Search" id="__field0" liveChange="onSearch"/>
			<List noDataText="Drop list items here" id="invoiceList" items="{/ZinvoiceHSet}">
				<items>
					<ObjectListItem type="Navigation" counter="0" intro="intro text" markFavorite="false" markFlagged="false" showMarkers="false" id="__item3" title="{Vendorname}" number="{ItemValue}" numberUnit="{ItemCur}" press="onInvoicePress">
						<attributes>
							<ObjectAttribute active="false" id="__attribute0" text="{ItemDescription}"/>
						</attributes>
					</ObjectListItem>
				</items>
			</List>
		</content>
	</Page>
</mvc:View>