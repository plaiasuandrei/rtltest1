<mvc:View xmlns:mvc="sap.ui.core.mvc" controllerName="RTLtest1.controller.InvoiceD">
	<Page xmlns="sap.m" title="{Vendorname}" id="__page1" navButtonPress="onNavBack" showNavButton="true" backgroundDesign="Transparent" enableScrolling="false">
		<content>
			<IconTabBar id="__bar0" expanded="false">
				<items>
					<IconTabFilter id="__filter4" icon="sap-icon://customer" iconColor="Positive">
						<content>
							<ObjectListItem title="{Vendorname}" id="__item0" type="Active" number="{ItemValue}" showMarkers="false" markFlagged="false" markFavorite="false" numberUnit="eur{ItemCur}" counter="0">
								<attributes>
									<ObjectAttribute id="__attribute0" text="{ItemDescription}" active="false" />
									<ObjectAttribute id="__attribute0_copy" text="{CompanyCode}" active="false" />
								</attributes>
							</ObjectListItem>
						</content>
					</IconTabFilter>
					<IconTabSeparator id="__separator0" />
					<IconTabFilter id="__filter3" icon="sap-icon://attachment" count="3">
						<content>
							<StandardListItem title="List Item" id="__item4" type="Navigation" description="Description text" counter="0" />
							<StandardListItem title="List Item" id="__item4_copy" type="Navigation" description="Description text" counter="0" />
							<StandardListItem title="List Item" id="__item4_copy2" type="Navigation" description="Description text" counter="0" />
						</content>
					</IconTabFilter>
				</items>
			</IconTabBar>
			<SearchField id="searchpo" placeholder="Search PO" width="100%" search="onSearch" suggestionItems="{ path: 'ponumbers>/POCollection', sorter: { path: 'POId' } }" suggest="onSuggest" enableSuggestions="true">
				<suggestionItems>
					<SuggestionItem description="{path:'ponumber>POId'} {path:'ponumber&gt;PODescription'}" text="{ponumbers>POId} - {ponumbers>PODescription}" key="{ponumber>POId}" />
				</suggestionItems>
			</SearchField>
			<sap.ui.layout:VerticalLayout xmlns:sap.ui.layout="sap.ui.layout" id="__layout1" width="100%">
			    <sap.ui.layout:content>
			        <ObjectHeader title="Title" id="__header0_copy" visible="false" number="123" numberUnit="eur" iconActive="false" titleActive="true" introActive="true" intro="Intro text">
			            <attributes>
			                <ObjectAttribute id="__attribute0_copy4" text="{ItemDescription}" active="false" />
			                <ObjectAttribute id="__attribute0_copy5" text="{CompanyCode}" active="false" />
			                </attributes>
			            
			        </ObjectHeader>
			        <Title id="__title0" text="Title" width="100%" titleStyle="H1" textAlign="Center" level="H1" />
			        <Button id="reassignBtn" type="Emphasized" text="I want to move the item to" width="100%" />
			        <SearchField id="__field0" placeholder="Search for user" width="100%" />
			        <Button id="rejectBtnItemNotMine" class="verticalBox" type="Reject" text="Item not mine" width="100%" icon="sap-icon://decline" />
			    </sap.ui.layout:content>
			</sap.ui.layout:VerticalLayout>
			
		</content>
	<footer>
	    <Toolbar id="__toolbar2" width="100%">


	                <Button id="__button7" type="Accept" text="Set" width="100%" icon="sap-icon://accept" />
	               <ToolbarSpacer />
	               

	                <Button id="__button8" type="Reject" text="Reject    " width="100%" icon="sap-icon://cancel" iconFirst="false" />


	    </Toolbar>
	</footer>
	<subHeader>
	    <core:Fragment xmlns:core="sap.ui.core" id="__fragment0--__bar2" type="XML" fragmentName="RTLtest1.view.detailSubHeaderBar" />
	</subHeader>
	</Page>
</mvc:View>